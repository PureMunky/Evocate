{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "buildDockerDb",
      "type": "shell",
      "command": "./run-db.sh"
    },
    {
      "label": "build-api",
      "dependsOn": ["buildDockerDb"],
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/Tete.Api/Tete.Api.csproj"],
      "problemMatcher": "$tsc"
    },
    {
      "label": "build-web",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/Tete.Web/Tete.Web.csproj"],
      "problemMatcher": "$tsc"
    }
  ]
}
