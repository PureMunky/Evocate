{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "buildDockerDb",
      "dependsOn": [
        "build-web"
      ],
      "type": "shell",
      "command": "./run-db.sh"
    },
    {
      "label": "build-web",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/Tete.Web/Tete.Web.csproj"
      ],
      "problemMatcher": "$tsc"
    }
  ]
}