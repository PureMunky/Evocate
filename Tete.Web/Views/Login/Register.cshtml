@model Tete.Models.Authentication.RegistrationResponse;
@{
  Layout = "_Layout";
}
<form action="/Login/Register" method="post" id="registrationForm">
  <h2>Register New Account</h2>
  @if(Model != null){
    <ul>
    @foreach(String s in Model.Messages){
      <li>@s</li>
    }
    </ul>
  }
  <section>
    <div>
      <label for="userName">UserName</label>
      <input type="textbox" name="userName" value="@(Model != null ? Model.Attempt.UserName : "")" />
    </div>
    <div>
      <label for="userDisplayName">Name</label>
      <input type="textbox" name="userDisplayName" value="@(Model != null ? Model.Attempt.DisplayName : "")"/>
    </div>
    <div>
      <label for="userEmail">Email</label>
      <input type="textbox" name="userEmail" value="@(Model != null ? Model.Attempt.Email : "")"/>
    </div>
    <div>
      <label for="userPassword">Password</label>
      <input type="password" name="userPassword" />
    </div>
    <div>
      <a href="/Login">Login</a>
    </div>
    <div>
      <input id="submit" type="submit" onsubmit="testForm()">
    </div>
  </section>
</form>